@model OrderViewModel

@{
    ViewData["Title"] = "MyOrders";
}

<h3>Orders created by me:</h3>

<div class="row mt-5">
    <div class="col-lg-6">
        @if (Model != null)
        {
            if (Model.SearchResults.Count == 0)
            {
                <p class="alert alert-info">There are no orders created by me yet!</p>
            }
            else
            {
                foreach (var order in Model.SearchResults)
                {
                    <partial name="_OrderPartial" model="order" />
                }
            }
        }
    </div>
</div>

@section Scripts
    {
    <partial name="_ValidationScriptsPartial" />
}