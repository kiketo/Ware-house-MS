@model OrderViewModel

@{
    ViewData["Title"] = "ChooseWarehouse";
}

<h2>Choose Warehouse</h2>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="ChooseWarehouse">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Warehouse" class="control-label"></label>
                <select asp-for="Warehouse" class="form-control" asp-items="Model.Warehouses"></select>
                <span asp-validation-for="Warehouse" class="text-danger"></span>
            </div>
            <div class="form-group">
                
                <input type="submit" value="ChooseWarehouse" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href='javascript:history.go(-1)'>Go Back</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}